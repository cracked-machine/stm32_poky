# POKY_BBLAYERS_CONF_VERSION is increased each time build/conf/bblayers.conf
# changes incompatibly
POKY_BBLAYERS_CONF_VERSION = "2"

BBPATH = "${TOPDIR}"
BBFILES ?= ""

BBLAYERS ?= " \
  /workspaces/stm32_poky/layers/poky/meta \
  /workspaces/stm32_poky/layers/poky/meta-poky \
  /workspaces/stm32_poky/layers/poky/meta-yocto-bsp \
  /workspaces/stm32_poky/layers/meta-test \
  /workspaces/stm32_poky/layers/meta-st-stm32mp \
  /workspaces/stm32_poky/layers/meta-openembedded/meta-python \
  /workspaces/stm32_poky/layers/meta-openembedded/meta-oe \
  "
